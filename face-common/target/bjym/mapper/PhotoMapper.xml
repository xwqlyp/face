<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.bjym.dao.PhotoMapper">
	<!-- 查询所有图片 -->
	<insert id="saveFace" parameterType="Face">
	
		INSERT INTO `face` (`id`, `PhotoName`, `PhotoImg` ,`type`) VALUES (#{id},#{PhotoName},#{PhotoImg},#{type});
		
	</insert>








	
	<insert id="saveFace_List" parameterType="Face_List">
		INSERT INTO `face_list` 
		(`id`, `beauty`,`face_token`, `age`, `emotion_probability`, `emotion_type`, `race_probability`,`race_type`,
		 `expression_probability`, `expression_type`, `face_shape_probability`, `face_shape_type`, 
		 `face_type_probability`, `face_type_type`, `gender_probability`,
		  `gender_type`, `glasses_probability`, `glasses_type`, `location_left`, `location_top`,
		   `location_width`, `location_height`, `location_rotation`, `occlusion_right_eye`, 
		   `occlusion_nose`, `occlusion_mouth`, `occlusion_left_eye`, `occlusion_left_cheek`, 
		   `occlusion_chin_contour`, `occlusion_right_cheek`, `quality_blur`, `quality_illumination`, 
		   `quality_completeness`, `eye_status_left_eye`, `eye_status_right_eye`) 
		   
		   
		   VALUES (#{id},#{beauty},#{face_token},#{age},#{emotion_probability},#{emotion_type},#{race_probability},#{race_type}, 
		  #{expression_probability},#{expression_type},#{face_shape_probability},#{face_shape_type},
		  #{face_type_probability},#{face_type_type},#{gender_probability},
		  #{gender_type}, #{glasses_probability},#{glasses_type},#{location_left},#{location_top},
		  #{location_width},#{location_height},#{location_rotation},#{occlusion_right_eye},
		  #{occlusion_nose},#{occlusion_mouth},#{occlusion_left_eye},#{occlusion_left_cheek},
		  #{occlusion_chin_contour},#{occlusion_right_cheek},#{quality_blur},#{quality_illumination},
		  #{quality_completeness},#{eye_status_left_eye},#{eye_status_right_eye})
	</insert>





















	<insert id="insert" parameterType="Face">
		INSERT INTO face (id,
		PhotoName, PhotoImg) VALUES
		("${id}", "${PhotoName}", "${PhotoImg}");

		INSERT INTO face_list (id, beauty, age) VALUES
		("${id}",
		"${face_list.beauty}",
		"${face_list.age}");

		INSERT INTO emotion (probability, type, id) VALUES
		("${face_list.emotion.probability}",
		"${face_list.emotion.type}",
		"${id}");

		INSERT INTO expression (probability, type, id) VALUES
		("${face_list.expression.probability}",
		"${face_list.expression.type}",
		"${id}");


	</insert>






</mapper>
